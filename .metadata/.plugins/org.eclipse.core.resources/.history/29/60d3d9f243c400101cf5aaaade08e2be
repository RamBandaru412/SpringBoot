package com.springboot.Learn_SpringJpaHibernate.course;

import org.springframework.boot.CommandLineRunner;
import org.springframework.stereotype.Component;

import com.springboot.Learn_SpringJpaHibernate.course.springdatajpa.CourseSpringDataJpa;

@Component
public class CourseCommandLineRunner implements CommandLineRunner{
	
	private CourseSpringDataJpa repository;

	@Override
	public void run(String... args) throws Exception {
		repository.save(new Course(1,"AWS","RAM"));
		repository.
		
	}

}
